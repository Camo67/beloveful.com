	<span class="pl-s1">is</span><span class="pl-kos">.</span><span class="pl-en">arrayLike</span><span class="pl-kos">(</span><span class="pl-smi">arguments</span><span class="pl-kos">)</span><span class="pl-kos">;</span>
	<span class="pl-c">//=&gt; true</span>
